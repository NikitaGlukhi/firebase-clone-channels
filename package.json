{
  "name": "firebase-clone-channels",
  "version": "1.1.0",
  "description": "This is the simple action, that allow you clone channels throw Firebase projects",
  "main": "bin/action.min.js",
  "source": "src/index.ts",
  "scripts": {
    "test": "npm run test",
    "build:microbundle": "microbundle --format cjs --target node --no-compress --no-sourcemap src/index.ts",
    "build": "pretty-quick --staged && npm run build:microbundle"
  },
  "keywords": [
    "firebase",
    "actions",
    "githib",
    "channels",
    "clone",
    "cloning"
  ],
  "author": "Nikita Glukhi <glukhi.nikita@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@tsconfig/node12": "^1.0.11",
    "@types/node": "^18.16.0",
    "@types/tmp": "^0.2.3",
    "husky": "^8.0.3",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "tmp": "^0.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  }
}
